{
  "name": "xdotool",
  "version": "1.0.55",
  "description": "C++ bindings for Node.js to handle X11 using the full power of libxdo.",
  "main": "./out/index.js",
  "typings": "./out/index.d.ts",
  "scripts": {
    "postinstall": "npx cmake-js --prefer-clang --out build/cmake-js compile"
  },
  "files": [
    "**/*.{cpp,h,c,map,js,d.ts}",
    "**/CMakeLists.txt",
    "!build",
    "!cmake-build-*"
  ],
  "keywords": [
    "x11",
    "xorg",
    "bindings"
  ],
  "author": {
    "email": "victorcqueirozg@gmail.com",
    "name": "Victor Queiroz",
    "url": "https://victorqueiroz.dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.14.2",
    "sarg": "5.0.12",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "bindings": "1.5.0",
    "cmake-js": "7.3.0",
    "nan": "2.20.0",
    "tslib": "2.6.3"
  }
}
